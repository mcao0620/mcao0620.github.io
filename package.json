{
  "name": "tailwind_static",
  "version": "1.0.0",
  "description": "",
  "main": "postcss.config.js",
  "devDependencies": {
    "@11ty/eleventy": "^3.1.2",
    "@tailwindcss/postcss": "^4.1.18",
    "@tailwindcss/typography": "^0.5.19",
    "autoprefixer": "^10.4.23",
    "browser-sync": "^3.0.4",
    "cssnano": "^7.1.2",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "tailwindcss": "^4.1.18"
  },
  "scripts": {
    "build:css": "NODE_ENV=production postcss src/styles.css -o dist/styles.css",
    "build:11ty": "eleventy",
    "build": "npm run build:11ty && npm run build:css",
    "dev:css": "postcss src/styles.css -o dist/styles.css --watch",
    "dev:11ty": "eleventy --serve --port=3000 --watch",
    "dev": "npm run build:css && run-p dev:css dev:11ty"
  },
